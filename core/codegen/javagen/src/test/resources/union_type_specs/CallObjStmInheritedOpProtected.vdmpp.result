import org.overture.codegen.runtime.*;

import java.util.*;


@SuppressWarnings("all")
public class B extends A {
    public B() {
    }

    public Number go() {
        Object obj = new B();
        Object a = 'a';
        Number b = 2L;

        if (obj instanceof A) {
            return A.class.cast(obj).op(Character.class.cast(a), b);
        } else if (obj instanceof B) {
            return B.class.cast(obj).op(Character.class.cast(a), b);
        } else {
            throw new RuntimeException("Missing operation member: op");
        }
    }

    public String toString() {
        return "B{}";
    }
}

