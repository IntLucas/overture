import org.overture.codegen.runtime.*;

import java.util.*;


@SuppressWarnings("all")
public class A {
    public A() {
    }

    protected Number op(final Character a, final Number b) {
        return 1L;
    }

    public String toString() {
        return "A{}";
    }
}

