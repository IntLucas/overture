import org.overture.codegen.runtime.*;

import java.util.*;


@SuppressWarnings("all")
public class Entry {
    public Entry() {
    }

    public static Object Run() {
        return new B().go();
    }

    public String toString() {
        return "Entry{}";
    }
}

