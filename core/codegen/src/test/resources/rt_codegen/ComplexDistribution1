class A

end A

system DistSys

instance variables
  cpu1 : CPU := new CPU(<FCFS>, 1E6);
  cpu2 : CPU := new CPU(<FCFS>, 1E6);
  cpu3 : CPU := new CPU(<FCFS>, 1E6);
  cpu4 : CPU := new CPU(<FCFS>, 1E6);

  bus1 : BUS := new BUS(<FCFS>, 1E3, {cpu1, cpu2, cpu3});
  bus2 : BUS := new BUS(<FCFS>, 1E3, {cpu1, cpu4});
  
  public static a1 	: A := new A();
  public static a2 	: A := new A();
  public static a3 	: A := new A();
  public static a4 	: A := new A();
  
operations
public DistSys: () ==> DistSys
DistSys() ==
 (
  cpu1.deploy(a1);
  cpu2.deploy(a2);
  cpu3.deploy(a3);
  cpu4.deploy(a4);
 );
  
end DistSys