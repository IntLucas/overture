import org.overture.codegen.javalib.*;

import java.util.*;


public class Entry {
    public Entry() {
    }

    public static Object Run() {
        return Test();
    }

    public static boolean pick() {
        return true;
    }

    public static VDMSeq Op() {
        VDMSet set_1 = SeqUtil.elems(SeqUtil.seq(1L, 2L, 3L));
        Iterator iterator_1 = set_1.iterator();
        VDMSeq seqCompResult_1 = SeqUtil.seq();

        while (iterator_1.hasNext()) {
            Number x = ((Number) iterator_1.next());

            if (pick()) {
                seqCompResult_1.add(x.longValue() * 2L);
            }
        }

        return seqCompResult_1;
    }

    public static Number Test() {
        Number sum = 0L;
        Iterator iterator_2 = Op().iterator();

        while (iterator_2.hasNext()) {
            Number e = (Number) iterator_2.next();
            sum = sum.longValue() + e.longValue();
        }

        return sum;
    }
}
