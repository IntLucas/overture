class A

values

private x : int = 1;

operations

public op : () ==> ()
op () == 
	let x0 = MATH`sin(x),
			x1 = MATH`cos(x),
			x2 = MATH`tan(x),
			x3 = MATH`cot(x),
			x4 = MATH`asin(x),
			x5 = MATH`acos(x),
			x6 = MATH`atan(x),
			x7 = MATH`acot(x),
			x8 = MATH`sqrt(x),
			x9 = MATH`pi_f(),
			--MATH`srand(x) 
			x10 = MATH`rand(x),
			x11 = MATH`srand2(x),
			x12 = MATH`exp(x),
			x13 = MATH`ln(x),
			x14 = MATH`log(x),
			x15 = MATH`fac(x),
			x16 = MATH`pi	
	in
		MATH`srand(x)

end A