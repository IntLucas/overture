abstract public class Employee {
    public abstract Number getSalary();
}

##########
public class HourlyPaidEmployee extends Employee {
    public Number hours;
    private Number rate;

    public HourlyPaidEmployee(Number pHours, Number pRate) {
        hours = pHours;
        rate = pRate;
    }

    public Number getSalary() {
        return hours.doubleValue() * rate.doubleValue();
    }

    public void SetRate(Number pRate) {
        if (pRate.doubleValue() > 150L) {
            rate = 150L;
        } else if (pRate.doubleValue() < 0L) {
            rate = 0L;
        } else {
            rate = pRate;
        }
    }
}

##########
public class FixedSalaryEmployee extends Employee {
    private Number fixedSalary;

    public FixedSalaryEmployee(Number pFixedSalary) {
        fixedSalary = pFixedSalary;
    }

    public Number getSalary() {
        return fixedSalary;
    }
}

