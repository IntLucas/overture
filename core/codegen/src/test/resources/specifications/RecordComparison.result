import utils.*;


public class A {
    public static final boolean a = new R(1L).equals(new R(2L));
    public static final boolean b = !(new R(1L).equals(new R(2L)));

    public A() {
    }

    public static class R implements Record {
        public Number x;

        public R(Number _x) {
            x = _x;
        }

        public boolean equals(Object obj) {
            if (!(obj instanceof R)) {
                return false;
            }

            R other = ((R) obj);

            return x == other.x;
        }

        public int hashCode() {
            return Utils.hashCode(x);
        }

        public R clone() {
            return new R(x);
        }

        public String toString() {
            return Utils.recordToString(this, x);
        }
    }
}
