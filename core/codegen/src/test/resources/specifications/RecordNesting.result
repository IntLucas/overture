import utils.*;


public class A {
    private static final R3 x = new R3(1L, new R2(new R1(2L)));
    private static final R3 y = x.clone();

    public static class R1 implements Record {
        public long x;

        public R1(long _x) {
            x = _x;
        }

        public R1 clone() {
            return new R1(x);
        }
    }

    public static class R2 implements Record {
        public R1 r;

        public R2(R1 _r) {
            r = _r.clone();
        }

        public R2 clone() {
            return new R2(r);
        }
    }

    public static class R3 implements Record {
        public long x;
        public R2 r;

        public R3(long _x, R2 _r) {
            x = _x;
            r = _r.clone();
        }

        public R3 clone() {
            return new R3(x, r);
        }
    }
}