\begin{vdm_al}
class AstMapType is subclass of AstType

instance variables
  private fromtp : AstType;
  private totp : AstType
  
operations
  public AstMapType: AstType * AstType ==> AstMapType
  AstMapType (pfrtp, ptotp) == ( fromtp := pfrtp; totp := ptotp );
  
  public getDomType: () ==> AstType
  getDomType () == return fromtp;
  
  public getRngType: () ==> AstType
  getRngType () == return totp;
  
  public accept: AstVisitor ==> ()
  accept (pv) == pv.visit(self);
  
  public isMapType: () ==> bool
  isMapType () == return true
  
end AstMapType
\end{vdm_al}