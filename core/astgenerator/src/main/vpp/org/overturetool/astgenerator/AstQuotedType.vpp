
\begin{vdm_al}
class AstQuotedType is subclass of AstType

instance variables
  private name : seq of char
  
operations
  public AstQuotedType: seq of char ==> AstQuotedType
  AstQuotedType (pname) == name := pname;
  
  public getName: () ==> seq of char
  getName () == return stringUpper(stripUnderscore(name));
  
  public accept: AstVisitor ==> ()
  accept (pv) == pv.visit(self);
    
  public isQuotedType: () ==> bool
  isQuotedType () == return true
  
end AstQuotedType
\end{vdm_al}
