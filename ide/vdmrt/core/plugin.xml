<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="nature"
         name="VDM-RT nature"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="org.overture.ide.vdmrt.core.VdmRtProjectNature">
         </run>
      </runtime>
   </extension>
   <extension
         point="org.eclipse.core.contenttype.contentTypes">
      <content-type
            base-type="org.eclipse.core.runtime.text"
            file-extensions="vdmrt"
            id="org.overture.ide.vdmrt.content-type"
            name="VDM-RT Content type"
            priority="high">
      </content-type>
         
         
   </extension>
   <extension
         point="org.eclipse.dltk.core.language">
      <language
            class="org.overture.ide.vdmrt.core.VdmRtLanguageToolkit"
            nature="org.overture.ide.vdmrt.core.nature"
            priority="0">
      </language>
   </extension>
  
</plugin>
