<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="nature"
         name="VDM-RT nature"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="org.overture.ide.vdmrt.core.VdmRtProjectNature">
         </run>
      </runtime>
   </extension>
   <extension
         point="org.eclipse.core.contenttype.contentTypes">
      <content-type
            base-type="org.eclipse.core.runtime.text"
            file-extensions="vdmrt"
            id="org.overture.ide.vdmrt.content-type"
            name="VDM-RT Content type"
            priority="high">
      </content-type>     
   </extension>
   <extension
         point="org.overture.ide.core.language">
      <Language
            dialect="VDM_RT"
            name="VDM-RT"
            nature="org.overture.ide.vdmrt.core.nature">
         <ContentType
               id="org.overture.ide.vdmrt.content-type">
         </ContentType>
      </Language>
   </extension>
  
</plugin>


