    <perspectiveExtension
      targetID="org.overture.ide.ui.perspective">
      <actionSet id="org.eclipse.debug.ui.breakpointActionSet"></actionSet>
    </perspectiveExtension>
  </extension>
  <extension
        point="org.eclipse.debug.ui.launchConfigurationTypeImages">
